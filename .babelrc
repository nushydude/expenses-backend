{
  "presets": [
    "@babel/preset-flow",
    [
      "@babel/preset-env",
      {
        "debug": true,
        "loose": true,
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
